---
layout: nil
---

<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
    <title>DreamCoder</title>
    <link href="http://blog.jachohx.com/"/>
    <link href="http://blog.jachohx.com/atom.xml" rel="self"/>
    <id>http://blog.jachohx.com/</id>
    <updated>{{ site.time | date_to_xmlschema }}</updated>

    {% for post in site.posts limit:20 %}
        <entry>
            <id>http://blog.jachohx.com{{post.id}}</id>
            <title>{{ post.title | xml_escape }}</title>
            <link href="http://qixinglu.com{{post.url}}"/>
            <updated>{{ post.date | date_to_xmlschema }}</updated>
            <author>
                <name>muzuiget</name>
            </author>
            <content type="html">{{ post.content | xml_escape }}</content>
        </entry>
    {% endfor %}
 
</feed>
